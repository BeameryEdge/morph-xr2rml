@prefix xrr: <http://i3s.modalis.xr2Rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ex: <http://example.com/> .
    
<#Sport>
    a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "sport"; ];
    rr:subjectMap [ 
    	rr:template "http://xr2rml/sport/{id}";
		rr:class ex:Sport;
    ].
    
<#Students>
    a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "student"; ];
    rr:subjectMap [ 
    	rr:template "http://xr2rml/student/{id}";
		rr:class ex:Student;
    ];
    rr:predicateObjectMap [
    	rr:predicate ex:practices;
    	rr:objectMap [
    	    rr:parentTriplesMap <#Sport>;
    	    rr:joinCondition [
    	        rr:child "sport";
    	        rr:parent "id";
    	    ];
    	    rr:termType xrr:RdfList;
    	];
    ].
    